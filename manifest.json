{
"name": "Journal App Services",
"short_name": "JAS",
"description": "Prototype for Wiley Journal App Services",
"version": "0.0.0.1",
"manifest_version": 2,
"default_locale": "en-GB",
"author": "Stuart Allen",
"start_url": "index.html",
"display": "standalone",
"orientation": "portrait",
"theme_color": "#005274",
"background_color": "#005274",
"icons": [
{
  "src": "assets/img/32x32.png",
  "sizes": "32x32",
  "type": "image/png"
},
{
  "src": "assets/img/192x192.png",
  "sizes": "192x192",
  "type": "image/png"
},
{
  "src": "assets/img/512x512.png",
  "sizes": "512x512",
  "type": "image/png"
}
],
"app": {
 "urls": [
   "https://sallen-wiley.github.io/jas/index.html"
 ],
 "launch": {
   "web_url": "https://sallen-wiley.github.io/jas/index.html"
 },
 "background": {
   "scripts": [ "chrome.js" ]
 },
 "permissions": [
  "unlimitedStorage",
  "notifications",
  "fullscreen"
 ]
}
}
