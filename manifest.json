{
"name": "MyApp",
"short_name": "MyApp",
"description": "MyApp description",
"version": "0.0.0.1",
"manifest_version": 2,
"default_locale": "en-GB",
"author": "Stuart Allen",
"start_url": "Home.aspx",
"display": "standalone",
"orientation": "portrait",
"theme_color": "#015174",
"background_color": "#F7F4F3",
"icons": [
{
  "src": "images/app-icon-chrome.png",
  "sizes": "128x128",
  "type": "image/png"
},
{
  "src": "images/app-icon-tiny.png",
  "sizes": "32x32",
  "type": "image/png"
},
{
  "src": "images/app-icon-192.png",
  "sizes": "192x192",
  "type": "image/png"
},
{
  "src": "images/app-icon-512.png",
  "sizes": "512x512",
  "type": "image/png"
}
],
"app": {
 "urls": [
   "https://sallen-wiley.github.io/jas/index.html"
 ],
 "launch": {
   "web_url": "https://sallen-wiley.github.io/jas/index.html"
 },
 "background": {
   "scripts": [ "chrome.js" ]
 },
 "permissions": [
  "unlimitedStorage",
  "notifications",
  "fullscreen"
 ]
}
}
